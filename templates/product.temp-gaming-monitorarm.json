    "img_with_foldtext_jdqEDW": {
      "type": "img-with-foldtext",
      "blocks": {
        "foldtext_hnrmGd": {
          "type": "foldtext",
          "settings": {
            "title": "<p>Wake Up to a Moment of Delight</p>",
            "description": "<p>Start your day with a quiet burst of color. The Blacklyte Pixel Display can show you the date in vibrant pixel form, turning a simple glance into something playful and personal. It’s a small detail—but the kind that adds warmth and energy to your daily rhythm.</p>"
          }
        },
        "foldtext_7iHCQ8": {
          "type": "foldtext",
          "settings": {
            "title": "<p>Art That Breathes with You</p>",
            "description": "<p>With every pixel, the Blacklyte Pixel Display brings life and movement into your space. You can upload custom animations or static designs to reflect your mood, your style, or the atmosphere you want to create. From setting a calm evening vibe to adding energy before a stream, the display becomes a visual extension of your identity—always shifting, always personal.</p>"
          }
        },
        "foldtext_UWV3Kh": {
          "type": "foldtext",
          "settings": {
            "title": "<p>Create, Animate, Repeat</p>",
            "description": "<p>The Blacklyte app is built for artists, tinkerers, and everyone in between. Design original pixel visuals, animate your ideas, or remix community favorites. Whether it’s mood-based art or a statement piece scrolling across the screen, your creativity drives the experience.</p>"
          }
        },
        "foldtext_Dgzbgj": {
          "type": "foldtext",
          "settings": {
            "title": "<p>Visuals That Match Your Vibe</p>",
            "description": "<p>Sync the display with movies, music, or gaming sessions for an immersive glow that fits the moment. Whether you're setting the tone for a late-night grind or just vibing with a playlist, the Blacklyte Pixel Display reacts in real time, turning your room into a living canvas.</p>"
          }
        },
        "foldtext_XR4hPe": {
          "type": "foldtext",
          "settings": {
            "title": "<p>Compact, Light, Built to Move</p>",
            "description": "<p>The Blacklyte Pixel Display is slim, solid, and surprisingly portable. Whether it's propped up on a desk, mounted on a wall, or packed for a gaming event—it goes wherever your setup needs it. No fuss, no clutter—just pure visual personality, anywhere.</p>"
          }
        }
      },
