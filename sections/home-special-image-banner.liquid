            #halo-image-banner-{{ section.id }} .column-2 .type-item--custom-2 {margin-top: 30px}
        }
        @media (max-width: 1024px) {
            #halo-image-banner-{{ section.id }} .video-open-popup--ct-pos .video-button {top: var(--top-mb);left: var(--left-mb)}   
