            #halo-image-banner-{{ section.id }} .content-box__col-1--left,#halo-image-banner-{{ section.id }} .content-box__col-2--left {margin-right: auto}
            #halo-image-banner-{{ section.id }} .content-box__col-1--right, #halo-image-banner-{{ section.id }} .content-box__col-1--right { margin-left: auto}
            #halo-image-banner-{{ section.id }} .content-box__col-1--center, #halo-image-banner-{{ section.id }} .content-box__col-1--center { margin-right: auto;margin-left: auto}
