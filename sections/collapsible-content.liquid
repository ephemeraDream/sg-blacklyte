<link rel="stylesheet" href="{{ 'component-faqs.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'component-faqs.css' | asset_url | stylesheet_tag }}</noscript>

{%- if section.blocks.size > 0 -%}
	<style>
	    {%- liquid
	        assign collapsible_bg = section.settings.collapsible_bg
	        assign collapsible_bg_gradient = section.settings.collapsible_bg_gradient

	        assign color_title = section.settings.color_title
	        assign fontsize_title = section.settings.fontsize_title
	        assign fontsize_title_mb = section.settings.fontsize_title_mb

	        assign color_des = section.settings.color_des
	        assign fontsize_des = section.settings.fontsize_des
	        assign fontsize_des_mb = section.settings.fontsize_des_mb
	        assign max_width_des = section.settings.max_width_des

	        assign mg_top_desktop = section.settings.mg_top_desktop
	        assign mg_top_tablet = section.settings.mg_top_tablet
	        assign mg_top_mobile = section.settings.mg_top_mobile

	        assign mg_bottom_desktop = section.settings.mg_bottom_desktop
	        assign mg_bottom_tablet = section.settings.mg_bottom_tablet
	        assign mg_bottom_mobile = section.settings.mg_bottom_mobile

	        assign image_right = section.settings.image_right
	        assign enable_image_right = section.settings.enable_image_right
	        assign bg_content_color = section.settings.bg_content_color
	        assign title_color = section.settings.title_color
	        assign content_color = section.settings.content_color
	        assign icon_color = section.settings.icon_color
	        assign border_color = section.settings.border_color
            assign font-size_heading = section.settings.font-size_heading
            assign font-size_heading_mb = section.settings.font-size_heading_mb
            assign font-size_content = section.settings.font-size_content
            assign font-size_content_mb = section.settings.font-size_content_mb
            assign font-size_content_line-height = section.settings.font-size_content_line-height
            assign font-size_content_line-height_mb = section.settings.font-size_content_line-height_mb
          
	    -%}

        .section-block-{{section.id}},
        .section-block-{{section.id}} .halo-block-header .title .text {
            background: {% if collapsible_bg_gradient != blank %}{{ collapsible_bg_gradient }}{% else %}{{ collapsible_bg }}{% endif %};
      
        }

	    .section-block-{{section.id}} .halo-block-header .title {
	        color: {{ color_title }};
	        font-size: {{ fontsize_title | append: 'px'  }};
	        line-height: 1;
    		margin-bottom: 40px;
	    }

	    .section-block-{{section.id}} .halo-block-header .title:before {
	        display: none;
	    }

	    .section-block-{{section.id}} .halo-block-header .rte {
	        font-size: {{ fontsize_des | append: 'px' }};
	        color: {{ color_des }};
	        max-width: {{ max_width_des | append: 'px' }};
	    }

	    .section-block-{{section.id}} .halo-block-header.text-right .title {
	    	display: block;
	    }

	    .section-block-{{section.id}} .halo-block-header.text-center .rte,
	    .section-block-{{section.id}} .halo-block-header.text-right .rte {
	    	display: inline-block;
	    }

	    .section-block-{{section.id}} .collapsible-content {
	    	display: flex;
		    width: 80%;
		    align-items: flex-start;
		    justify-content: space-between;
		    margin:0 auto;
	    }

	    .section-block-{{section.id}} .collapsible-grid {
	    	width: 47.14%;
	    }

	    .section-block-{{section.id}} .not_image_right .collapsible-grid {
	    	width: 100%;
	    }

	    .section-block-{{section.id}} .collapsible-grid .faqs-item {
	    	background-color: {{ bg_content_color }};
	    }

	    .section-block-{{section.id}} .collapsible-grid .faqs-item+.faqs-item {
	    	margin-top: 0;
	    }

	    .section-block-{{section.id}} .collapsible-grid .collapsible-grid__item {
	    	border-bottom: 1px solid rgba(255, 255, 255, 0.2);
	    }

	    .section-block-{{section.id}} .collapsible-grid .card-header {
	    	border-top: 1px solid rgba(255, 255, 255, 0.2);
	    }

	    .section-block-{{section.id}} .collapsible-grid .faqs-item .title {
	    	color: {{ title_color }};
	    	background-color: transparent;
	    	padding-left: 0;
	    	/* font-size: 13px; */
            font-size: {{ font-size_heading | append: 'px' }};
		    font-weight: 600;
		    text-transform: uppercase;
		    padding-top: 30px;
    		padding-bottom: 30px;
           line-height: 1.33; 
	    }

	    .section-block-{{section.id}} .collapsible-grid .faqs-item .title:after {
	    	content: none;
	    }

	    .section-block-{{section.id}} .collapsible-grid .card-header .arrow{
	    	border: none;
		    transform: none;
		    top: 27%;
	    }

	    .section-block-{{section.id}} .collapsible-grid .card-header .arrow svg{
	    	fill: {{ icon_color }};
		    width: 18px;
		    height: 18px;
		    transform: rotate(90deg);
	    }

	    .section-block-{{section.id}} .collapsible-grid .card-header:not(.collapsed) .arrow svg {
	    	transform: rotate(-90deg);
	    }

	    .section-block-{{section.id}} .collapsible-grid .faqs-item .card-body {
	    	color: {{ content_color }};
	    	padding-left: 0;
	    	margin-top: 0;
            font-size: {{ font-size_content | append: 'px' }};
            line-height: 1.4;
            /* line-height: {{ font-size_content_line-height | append: 'px' }}; */
          font-weight:500;
	    }

	    .section-block-{{section.id}} .card-body table {
	    	border-collapse: collapse;
    		border-spacing: 0;
    		width: 100%;
    		margin-bottom: 36px;
	    }

	    .section-block-{{section.id}} .card-body table tr:nth-child(odd) {
		    background: #121212;
		}

		.section-block-{{section.id}} .card-body table td {
			padding: 16px 10px 14px;
    		text-align: left;
		}

		.section-block-{{section.id}} .card-body table tr td:first-child {
			padding: 16px 30px 14px;
			max-width: 142px;
		}

	    .section-block-{{section.id}} .collapsible-image {
	    	width: calc(52.86% - 129px);
	    	margin-top: -56px;
	    	animation: moveUpDown 2s alternate infinite ease-in-out !important;
    		will-change: transform;
	    }
        .section-block-{{section.id}} .icon_before_title {
            vertical-align: bottom;
            margin-right: 14px;
        }
        .section-block-{{section.id}} .icon_before_title svg {
            width: 24px;
            height: 24px;
            fill: none;
            stroke: none;
        }

      
        /* fall final events */
        #fallfinal_events_container .collapsible-content {
          width: 100%;
        }
        #fallfinal_events_container .collapsible-grid__item {
          border: none;
        }
        #fallfinal_events_container .collapsible-grid .faqs-item {
          margin-bottom: 5px;
        }
        #fallfinal_events_container .collapsible-grid .faqs-item .card-body {
          line-height: 300px;
          padding-bottom: 0px;
        }
        #fallfinal_events_container .collapsible-grid .faqs-item .card-header {
          background-color: #362431;
          margin-bottom: 5px;
        }
        #fallfinal_events_container .collapsible-grid .faqs-item .title {
          padding-top: 15px;
          padding-bottom: 15px;
          text-indent: 15px;
        }
        #fallfinal_events_container .collapsible-grid .card-header {
          border: none;
        }
        #fallfinal_events_container .halo-block-content .collapsible-content .collapsible-grid .collapsible-grid__item .faqs-item:nth-of-type(1) .collapse {
          background: url('https://cdn.shopify.com/s/files/1/0663/6742/2695/files/fallfinal_teams_groupa.png?v=1730427099') no-repeat;
          background-size: contain;
          background-position: center;
        }
         
        #fallfinal_events_container .halo-block-content .collapsible-content .collapsible-grid .collapsible-grid__item .faqs-item:nth-of-type(2) .collapse {
          background: url('https://cdn.shopify.com/s/files/1/0663/6742/2695/files/fallfinal_teams_groupb.png?v=1730427234') no-repeat;
          background-size: contain;
          background-position: center;
        }
        #fallfinal_events_container .halo-block-content .collapsible-content .collapsible-grid .collapsible-grid__item .faqs-item:nth-of-type(3) .collapse {
          background: url('https://cdn.shopify.com/s/files/1/0663/6742/2695/files/fallfinal_teams_playoffs.png?v=1730688118') no-repeat;
          background-size: contain;
          background-position: center;
        }
        /* FAQ 模块 */
        #resources-container-sectionID .container-full {
          width: 70%;
          margin: 0 auto;
        }
        #resources-container-sectionID .halo-block-header .title .text {
          /* font-family: "Orbitron"; */
          font-size: 30px;
          font-weight: 500;
          color: #18181B;
        }
        #resources-container-sectionID .collapsible-grid .faqs-item .title {
          /* font-family: "Poppins"; */
          font-size: 18px;
          font-weight: 500;
          color: #18181B;
          background-color: #F2F2F2;
          padding-top: 18px;
          padding-bottom: 18px;
          display: flex;
        }
        #resources-container-sectionID .faqs .faqs-item .card-header:not(.collapsed)+.collapse {
          border: 0.5px solid #D4D4D8;
        }
        #resources-container-sectionID .collapsible-grid .faqs-item {
          margin-bottom: 6px;
        }
        #resources-container-sectionID .collapsible-grid .faqs-item .card-body {
          /* font-family: "Poppins"; */
          font-size: 14px;
          line-height: 30px;
          font-weight: 400;
          color: #52525B;
          padding-left: 54px;
          padding-right: 54px;
        }
        #resources-container-sectionID .icon_before_title {
          margin-left: 19px;
          margin-right: 19px;
          display: block;
        }
        #resources-container-sectionID .icon_before_title svg {
          width: 16px;
          height: 16px;
        }
        #resources-container-sectionID .collapsible-content {
          width: 90%;
        }
        
        @media (min-width: 2000px) {
          .section-block-{{section.id}} .halo-block-header .title {
            font-size: {{ fontsize_title |divided_by:1920.00|times:100.00 | append: 'vw'  }}
          }
          .section-block-{{section.id}} .collapsible-grid .faqs-item .title {
            font-size: {{ font-size_heading |divided_by:1920.00|times:100.00 | append: 'vw'  }}
          }
	    .section-block-{{section.id}} .collapsible-grid .faqs-item .card-body {
            font-size: {{ font-size_content |divided_by:1920.00|times:100.00 | append: 'vw' }};
              line-height: 1.4;
        }
          
        }
	    @media (max-width: 1400px) {
	    	.section-block-{{section.id}} .collapsible-image {
		    	width: calc(52.86% - 40px);
		    	margin-top: -10px;
		    }
	    }

	    @media (max-width: 1024px) {
	    	.section-block-{{section.id}} .collapsible-image {
		    	width: calc(52.86% - 20px);
		    	margin-top: 0;
		    }

	    	.section-block-{{section.id}} .card-body table tr td:first-child {
				padding: 16px 10px 14px;
				max-width: 100%;
			}

			.section-block-{{section.id}} .card-body table {
	    		margin-bottom: 0;
		    }

		    .section-block-{{section.id}} .collapsible-grid .card-header .arrow svg{
			    width: 10px;
			    height: 10px;
		    }
	    }

	    @media (max-width: 991px) {
	    	.section-block-{{section.id}} .collapsible-content {
		    	flex-direction: column-reverse;
		    }

		    .section-block-{{section.id}} .collapsible-image {
		    	width: 60%;
                margin-bottom: 60px;
                margin-left: auto;
                margin-right: auto;
		    }

		    .section-block-{{section.id}} .collapsible-grid {
	    		width: 100%;
	    	}

	    	.section-block-{{section.id}} .collapsible-image img {
	    		width: 100%;
    			margin: 0 auto;
	    	}
	    }
        .section-block-{{section.id}} .halo-block-header .title .text{
                line-height:1.2;
            }
	    @media (max-width: 991px) {
	        .section-block-{{section.id}} {
	            padding-top: {{ mg_top_mobile | append: 'px' }};
	            padding-bottom: {{ mg_bottom_mobile | append: 'px' }};
	        }

	        .section-block-{{section.id}} .halo-block-header .title {
	            font-size: {{ fontsize_title_mb | append: 'px'  }};
	            margin-bottom: 20px;
	        }
	        .section-block-{{section.id}} .halo-block-header .title .text{
                line-height:1.28;
            }
	        .section-block-{{section.id}} .halo-block-header .rte {
		        font-size: {{ fontsize_des_mb | append: 'px' }};
		        line-height: 24px;
		    }

            .section-block-{{section.id}} .collapsible-image {
		    	width: 85%;
		    }

		    .section-block-{{section.id}} .collapsible-image img {
	    		width: 100%;
	    	}

	    	.section-block-{{section.id}} .collapsible-grid .faqs-item .title {
			    padding-top: 16px;
	    		padding-bottom: 16px;
		    }

		    .section-block-{{section.id}} .collapsible-grid .card-header .arrow{
	    		top: 25%;
		    }

		    .section-block-{{section.id}} .collapsible-grid .faqs-item .card-body {
		    	padding-bottom: 20px;
		    }

		    .cart-count-bubble, .wishlist-count-bubble {
		    	font-size: calc(var(--font-body-size) - 2px);
		    }
          .section-block-{{section.id}} .collapsible-content.faqs {
            flex-direction: column-reverse !important;
          }
	    }

	    @media (min-width: 1200px) {
	        .section-block-{{section.id}} {
	            padding-top: {{ mg_top_desktop | append: 'px' }};
	            padding-bottom: {{ mg_bottom_desktop | append: 'px' }};
	        }
	    }

	    @media (min-width: 1025px) {
	        {% if section.settings.container == 'fullwidth' %}
	            .section-block-{{section.id}} .container-full {
	                padding-left: {{ padding_full_width | append: 'px' }};
	                padding-right: {{ padding_full_width | append: 'px' }};
	            }
	        {% endif %}
	    }

	    @media (min-width: 992px) and (max-width: 1199px) {
	        .section-block-{{section.id}} {
	            padding-top: {{ mg_top_tablet | append: 'px' }};
	            padding-bottom: {{ mg_bottom_tablet | append: 'px' }};
	        }
	    }
        #aboutus-collapsibel-id {
          width: 840px;
          margin: 0 auto;
          margin-bottom: 80px;
        }
       #aboutus-collapsibel-id .container-1570 {
         max-width: 100%;
       }
       #aboutus-collapsibel-id .icon-plus.arrow {
         display: none;
       }
      #aboutus-collapsibel-id .collapse {
        display: none;
      }
      #aboutus-collapsibel-id .collapsible-grid .card-header {
        border: none;
        font-weight: 400;
        display: flex;
      }
      #aboutus-collapsibel-id .collapsible-grid .faqs-item .title {
        font-weight: 400;
        padding: 10px 10px 36px 10px;
      }
       @media (max-width: 991px) {
         #aboutus-collapsibel-id {
           margin-bottom: 10px;
         }
         .section-block-{{section.id}} .collapsible-grid .faqs-item .card-body {
            font-size: {{ font-size_content_mb | append: 'px' }};
            line-height: {{ font-size_content_line-height_mb | append: 'px' }};
	     }
         .section-block-{{section.id}} .collapsible-grid .faqs-item .title {
            font-size: {{ font-size_heading_mb | append: 'px' }};
            line-height:1.5;
	     }
       }

      @media(max-width: 767px) {
        .section-block-{{section.id}} .collapsible-content .title {
          padding-left: 10px;
          display: flex;
        }
        .section-block-{{section.id}} .icon_before_title {
          vertical-align: baseline;
          margin-right: 6px;
        }
        .section-block-{{section.id}} .icon_before_title svg {
          width: 12px;
          height: 12px;
        }
        /* FAQS */
        #resources-container-sectionID .container-full {
          width: 90%;
        }
        #resources-container-sectionID .collapsible-content {
          width: 100%;
        }
        #resources-container-sectionID .halo-block-header .title .text {
          font-size: 24px;
        }
        #resources-container-sectionID .collapsible-grid .faqs-item .title {
          font-size: 14px;
        }
        #resources-container-sectionID .collapsible-grid .faqs-item .card-body {
          font-size: 12px;
          padding-left: 14px;
          line-height: 18px;
          padding-right: 14px;
        }
        #resources-container-sectionID .icon_before_title {
          margin-left: 10px;
          margin-right: 10px;
        }
      }
      /* 1025px ~ 1900px */
      @media(min-width: 1025px) and (max-width: 1900px) {
        #resources-container-sectionID .container-full {
          width: 90%;
        }
      }
       /* 768px ~ 1024px */
      @media(min-width: 768px) and (max-width: 1024px) {
        #resources-container-sectionID .container-full {
          width: 90%;
        }
      }
	</style>
    <faqs-item class="halo-collapsible-content section-block-{{section.id}}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}"{% if section.settings.id_section != blank %} id="{{ section.settings.id_section }}"{% else %} id="halo-collapsible-content-{{ section.id }}"{% endif %}>
    	<div class="{% if section.settings.container == 'fullwidth' %}container-full{% elsif section.settings.container == '1770' %}container-1770{% elsif section.settings.container == '1570' %}container-1570{% elsif section.settings.container == '1170' %}container-1170{% else %}container{% endif %}">
    		 {% if section.settings.collapsible_title != blank or section.settings.collapsible_des != blank %}
                <div class="halo-block-header text-{{ section.settings.title_align }}">
                    {% if section.settings.collapsible_title != blank %}
                        <h3 class="title">
                            <span class="text">
                            {{ section.settings.collapsible_title }}
                            </span>
                        </h3>
                    {% endif %}
                    {% if section.settings.collapsible_des != blank %}
                        <div class="rte">
                            {{ section.settings.collapsible_des }}
                        </div>
                    {% endif %}
                </div>
            {% endif %}
            <div class="halo-block-content">
            	<div class="collapsible-content faqs{% unless enable_image_right %} not_image_right{% endunless %}">
            		<div class="collapsible-grid">
	            		<div class="collapsible-grid__item">
	            			{%- for block in section.blocks -%}
	            				{%- liquid
            						assign heading = block.settings.heading
                                    assign content = block.settings.content
                                    assign content_page = block.settings.page.content
                                    assign show_header_title_before_icon = block.settings.show_header_title_before_icon
                                    
	            				-%}
                                
  	            				  {%- if content != blank or content_page != blank -%}
		            				<div class="faqs-item{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                                        <div class="card" id="#accordion-{{ block.id }}-{{ forloop.index }}">
                                            <div class="card-header {% unless forloop.first %}collapsed{% endunless %}" id="question-{{ block.id }}-{{ forloop.index }}" data-toggle="collapse" data-target="#collapse-{{ block.id }}-{{ forloop.index }}" aria-expanded="true" aria-controls="collapse-{{ block.id }}-{{ forloop.index }}" role="button">
                                                {%- if show_header_title_before_icon -%}
                                                  <span style="padding-top: 10px;">
                                                    <svg style="width: 24px;height: 24px;fill: none;stroke: none;" width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                      <path d="M7 13L10 16L17 9" stroke="#52525B" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                                    </svg>
                                                  </span>
                                                {%- endif -%}
                                                <div class="title" role="button" tabindex="0">
                                                    {%- if block.settings.header_title_before_iconSvg != blank -%}
                                                      <span class="icon_before_title">
                                                        {{ block.settings.header_title_before_iconSvg }}
                                                      </span>
                                                    {%- endif -%}
                                                    <span>{{ heading }}</span>
                                                </div>
                                                <span class="icon-plus arrow" role="none">
                                                  <svg aria-hidden="true" focusable="false" role="presentation" data-name="Layer 1" id="Layer_1" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg"><polygon points="150.46 478 129.86 456.5 339.11 256 129.86 55.49 150.46 34 382.14 256 150.46 478"/></svg>
      
                                                </span>
                                            </div>
                                            <div id="collapse-{{ block.id }}-{{ forloop.index }}" class="collapse" data-parent="#accordion"{% if forloop.first %} style="max-height: max-content"{% endif %}>
                                                <div class="card-body" id="answer-{{ block.id }}-{{ forloop.index }}">
                                                      {{ content }}
                                                      {{ content_page }}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                {%- endif -%}
	            			{%- endfor -%}
	            		</div>
	            	</div>
	            	{%- if enable_image_right != blank -%}
		            	<div class="collapsible-image">
			            	{%- if image_right != blank -%}
				                <a class="image image-custom image-adapt adaptive_height{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--fade-in{% endif %}"{% if button_link_block_bottom == blank %} role="link" aria-disabled="true"{% else %} href="{{ button_link_block_bottom }}"{% endif %} style="padding-top: {% unless image_right.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: image_right.aspect_ratio | times: 200 | append: '%' }}{% else %}{{ 1 | divided_by: image_right.aspect_ratio | times: 100 | append: '%' }}{% endunless %}" title="{{ image.alt | default: shop.name | escape }}">
				                {%- assign preview_img = image_right -%}
                                {%- assign height_dsk = preview_img.width | divided_by: preview_img.aspect_ratio | round  -%}
				                <img width="{{preview_img.width}}" height="{{height_dsk}}" {% render 'lazyload',preview_img: preview_img  %} />
				                {%- if settings.enable_lazyload -%}<span class="data-lazy-loading"></span>{%- endif -%}
				                </a>
				            {%- else -%}
				                <a class="image"{% if button_link_block_bottom == blank %} role="link" aria-disabled="true"{% else %} href="{{ button_link_block_bottom }}"{% endif %}>
				                    <div class="not_img" style="--height_not_img: 640px">813 x 640px</div>  
				                </a>
				            {%- endif -%}
			            </div>
		            {%- endif -%}
            	</div>
            </div>
    	</div>
    </faqs-item>
{%- endif -%}
<script src="{{ 'halo-faqs.js' | asset_url }}" defer></script>
{% schema %}
	{
		"name": "t:sections.collapsible_content.name",
		"settings": [
			{
		        "type": "header",
