







  <div class="oy-fold-faq">
        <div class="oy-fold-faq-in">
            <div class="oy-fold-faq-in-title">
                <p>{{ section.settings.title }}</p>
            </div>
            <div class="oy-fold-faq-in-main">
              {% for faq in section.blocks %}
                       <div class="oy-fold-faq-in-main-item">
                    <div class="oy-fold-faq-in-main-item-title">

                        <p>{{ faq.settings.title }}</p>
                        <div class="oy-svg-l">
                            <svg width="14" height="15" viewBox="0 0 14 15" fill="none"
                                xmlns="http://www.w3.org/2000/svg">
                                <path
                                    d="M7 1.11035C10.3125 1.11035 13 3.79785 13 7.11035C13 10.4229 10.3125 13.1104 7 13.1104C3.6875 13.1104 1 10.4229 1 7.11035C1 3.79785 3.6875 1.11035 7 1.11035ZM7 0.110352C3.1375 0.110352 0 3.24785 0 7.11035C0 10.9729 3.1375 14.1104 7 14.1104C10.8625 14.1104 14 10.9729 14 7.11035C14 3.24785 10.8625 0.110352 7 0.110352ZM7.5 11.1104H6.5V10.1104H7.5V11.1104ZM7.675 7.93535L7.5875 8.01035C7.5375 8.04785 7.5 8.13535 7.5 8.21035V9.12285H6.5V8.21035C6.5 7.82285 6.675 7.46035 6.9625 7.22285L7.05 7.14785C7.925 6.44785 8.35 6.08535 8.35 5.46035C8.35 4.71035 7.75 4.11035 7 4.11035C6.225 4.11035 5.65 4.68535 5.65 5.46035H4.65C4.65 4.14785 5.6875 3.11035 7 3.11035C8.3 3.11035 9.35 4.16035 9.35 5.46035C9.35 6.59785 8.575 7.22285 7.675 7.93535Z"
                                    fill="#999999" />
                            </svg>

                        </div>
                        <div class="oy-svg-r">
                            <svg width="22" height="13" viewBox="0 0 22 13" fill="none"
                                xmlns="http://www.w3.org/2000/svg">
                                <path d="M21 1.11035L11 11.1104L1 1.11035" stroke="#333333" stroke-width="2" />
                            </svg>

                        </div>
                    </div>
                    <div class="oy-fold-faq-in-main-item-content">
                        <div class="oy-fold-faq-in-main-item-content-in">
                               {{ faq.settings.text }}
                        </div>
                    </div>
                </div>
              {% endfor %}
         
            </div>
        </div>
    </div>



<style>
  .oy-fold-faq{
    padding-top:{{section.settings.p_t}}px;
    padding-bottom:{{section.settings.p_m}}px;
  }


</style>


                                  

    <script>
             let foldFaqItemAll = document.querySelectorAll(".oy-fold-faq-in-main-item")
            foldFaqItemAll.forEach((e, i) => {
            e.querySelector(".oy-fold-faq-in-main-item-title").addEventListener("click", e2 => {
        
                e.querySelector(".oy-fold-faq-in-main-item-content").classList.toggle("active")
            })
        })

    </script>




{% schema %}
  {
    "name": "A Fold FAQ",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "标题"
      },
           {
        "type":"range",
        "id": "p_t",
        "label": "Padding Top",
        "max":300,
        "min": 0,
        "default":30,
        "unit": "px",
        "step": 5,
      },
            {
        "type":"range",
        "id": "p_m",
        "label": "Padding Bottom",
        "max":300,
        "min": 0,
        "default":30,
        "unit": "px",
        "step": 5,
      },
    ],
    "blocks": [
      {
        "type": "faq",
        "name":"faq",
        "settings": [
          {
            "type": "text",
            "id":"title",
            "label": "标题"
          },
             {
            "type":"richtext",
            "id":"text",
            "label": "内容"
          },
        ]
      },
     
    ],
    "presets": [
    {"name":   "A Fold FAQ"}
    ]
  }
{% endschema %}