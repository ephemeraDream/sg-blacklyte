<style>
  .header_quick_links {
    padding: 0 45px;
    display: flex;
    justify-content: space-between;
    gap: 40px;
  }

  .header_quick_links .header_quick_links_left {
    flex: 0 0 auto;
  }

  .header_quick_links .header_quick_links_left_title {
    font-family: Roboto;
    font-weight: 500;
    font-size: 16px;
    line-height: 1.1875;
    letter-spacing: 0%;
    color: #b5b5b5;
    text-align: left;
  }

  .header_quick_links .header_quick_links_left_links {
    font-family: Roboto;
    font-weight: 500;
    font-size: 16px;
    line-height: 1.1875;
    letter-spacing: 0%;
    color: #4e4e4e;
    margin-top: 17px;
    display: block;
    text-align: left;
  }

  .header_quick_links .header_quick_links_left_links:hover {
    color: #429bff;
  }

  .header_quick_links .header_quick_links_right {
    flex: 1;
    display: flex;
    align-items: flex-start;
    gap: 27px;
    margin-top: 22px;
    max-width: 961px;
  }

  .header_quick_links .header_quick_links_right_item {
    flex: 0 0 calc((100% - 81px) / 4);
    display: flex;
    flex-direction: column;
  }

  .header_quick_links .header_quick_links_right_item_img {
    width: 100%;
    height: 145px;
    border-radius: 10px;
    background-color: #f2f2f2;
  }

  .header_quick_links .header_quick_links_right_item_img img {
    width: 100%;
    height: 100%;
    object-fit: contain;
    transition: transform 0.4s ease-in-out;
  }

  .header_quick_links .header_quick_links_right_item:hover .header_quick_links_right_item_img img {
    transform: scale(1.2);
  }

  .header_quick_links .header_quick_links_right_item_title {
    font-family: Roboto;
    font-weight: 500;
    font-size: 16px;
    leading-trim: Cap height;
    line-height: 20px;
    letter-spacing: 0%;
    text-align: center;
    color: #333333;
    margin-top: 16px;
  }

  .header_quick_links .header_quick_links_right_item:hover .header_quick_links_right_item_title {
    color: #429bff;
  }

  @media screen and (max-width: 768px) {
    .header_quick_links {
      padding: 0;
      flex-wrap: wrap;
    }

    .header_quick_links .header_quick_links_right {
      flex-wrap: wrap;
    }

    .header_quick_links .header_quick_links_left {
      flex: 0 0 100%;
    }

    .header_quick_links .header_quick_links_right {
      flex-wrap: wrap;
      width: 100%;
      gap: 20px;
    }

    .header_quick_links .header_quick_links_right_item {
      flex: 0 0 calc((100% - 20px) / 2);
    }
  }
</style>

<div class="header_quick_links">
  <div class="header_quick_links_left">
    <div class="header_quick_links_left_title">{{ 'templates.search.quick_links' | t }}</div>
    <a class="header_quick_links_left_links" href="/products/blacklyte-kraken-pro-gaming-chair">
