<script>
  (function ($) {
    $('.chairs_collections_title_icon').on('click', function () {
      $('.chairs_collections_mb').removeClass('menu_open_mb_active')
      $('body').removeClass('menu_open_mb')
      $('.chairs_collection_not_title').scrollTop(0)
      
    })
    $('.chairs_collection_not_title').on('scroll', function (e) {
      if (e.target.scrollTop > 0) {
        $('.chairs_collections_title').addClass('scrolled')
      } else {
        $('.chairs_collections_title').removeClass('scrolled')
      }
    })

    $('.chairs_collections_container').on('click', '.items', function (event) {
      event.preventDefault()
      event.stopPropagation()
      window.location.href = $(this).attr('data-href-id')
    })

    $('.chairs_collections_container .items .items_product_btns').on('click', 'span', function (event) {
      event.preventDefault()
      event.stopPropagation()
      const url = $(this).data('url')
      if(!url) return false
      window.location.href = url
    })
    
  })(jQuery)
  
</script>
