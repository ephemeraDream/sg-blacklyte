<style>
  .chairs_collections_mb {
    height: 100vh;
    width: 100%;
    max-height: 100vh;
    max-width: 100%;
    overflow: hidden;
    transition: .5s;
    position: absolute;
    top: 0;
    left: 100vw;
    opacity: 0;
    background-color: #fff;
    z-index: 999;
  }
  .chairs_collections_mb.menu_open_mb_active {
    left: 0;
    opacity: 1;
  }
  .chairs_collections_title {
    padding: 20px;
    /* font-family: "Poppins"; */
    border: 1px solid #f2f2f2;
    display: flex;
    align-items: center;
  }
  .chairs_collections_title.scrolled {
    /* box-shadow: 0 5px 10px -8px #000; */
    position: relative;
    z-index: 100;
  }
  .chairs_collections_title svg {
    width: 20px;
    height: 20px;
  }
  .chairs_collections_title.chairs_collections_title_sticky {
    position: sticky !important;
    top: 0;
    transition: opacity .1s, visibility .2s, transform .2s, -webkit-transform .2s, top .2s;
  }
  .chairs_collections_mb .chairs_collections_title_text {
    /* font-family: "Poppins"; */
    font-size: 20px;
    color: #333333;
    font-weight: 500;
    flex: 1;
    text-align: center;
    text-transform: uppercase;
  }
  .chairs_collections_mb .chairs_collection_not_title {
    height: calc(100vh - 62px);
    max-height: calc(100vh - 62px);
    overflow-y: scroll;
    padding-top: 20px;
  }
  .chairs_collections_container {
    display: flex;
    flex-wrap: wrap;
    padding: 0 20px 30px 20px;
  }
  .chairs_collections_container .items {
    width: 50%;
    height: auto;
    /* border: 1px solid #D0D0D0; */
    border-radius: 10px;
    cursor: pointer;
    padding-bottom: 20px;
    margin-bottom: 18px;
    text-align: center;
  }
  /* .chairs_collections_container .items:nth-child(even) {
    margin-left: 4%;
  } */
  .chairs_collections_container .items .items_product_image {
    width: 70%;
    height: auto;
    margin-left: auto;
    margin-right: auto;
  }
  .chairs_collections_container .items .items_product_image img {
    width: 100%;
    height: 100%;
    max-width: 100%;
    max-height: 100%;
  }
  .chairs_collections_container .items .items_product_title {
    /* font-family: "Orbitron"; */
    font-size: 15px;
    margin-top: 20px;
    margin-bottom: 12px;
    font-weight: 600;
    color: #333333;
    text-align: center;
  }
  .chairs_collections_container .items .items_product_btns span {
    /* font-family: "Poppins"; */
    font-size: 12px;
    font-weight: 400;
    color: #666;
    text-decoration: underline;
    text-underline-offset: 3px;
  }
  .chairs_collections_container .items .items_product_btns span.items_check_btn {
    margin-left: 10px;
  }
  .chairs_collections_mb .chairs_collection_not_title .chairs_collections_other_item {
    display: flex;
    padding: 0 14%;
    justify-content: space-between;
    align-items: center;
  }
  .chairs_collections_mb .chairs_collection_not_title .chairs_collections_other_item.chairs_collections_other_addonsitems {
    margin-top: 30px;
    margin-bottom: 100px;
  }
  body.iOS .chairs_collections_mb .chairs_collection_not_title .chairs_collections_other_item.chairs_collections_other_addonsitems {
    margin-bottom: 100px;
  }
  .chairs_collections_mb .chairs_collection_not_title .chairs_collections_other_item .name_text {
    /* font-family: "Poppins"; */
    font-size: 16px;
    font-weight: 500;
    color: #333;
    letter-spacing: 0;
  }
  .chairs_collections_mb .chairs_collection_not_title .chairs_collections_other_item .icons svg {
    width: 10px;
    height: 10px;
    fill: none;
    stroke: none;
  }
</style>
